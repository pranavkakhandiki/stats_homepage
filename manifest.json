{
    "manifest_version": 3,
    "name": "stats_homepage",
    "version": "0.1",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "chrome_url_overrides": {
        "newtab": "homepage.html"
    },
    
    "description": "A homepage informing you about your daily browsing",
    "permissions":[
        "history"
    ]
   


}