{
    "manifest_version": 2,
    "name": "stats_homepage",
    "version": "0.69",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background":{
        "page": "background.html"
    },
    
    "chrome_url_overrides": {
        "newtab": "homepage.html"
    },
    
    "description": "A homepage informing you about your daily browsing",
    "permissions":[
        "history"
    ]
}